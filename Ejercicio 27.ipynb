   "source": [
    "# 27. Crea una función que calcule el promedio de una lista de números.\n",
    "def promedio(lista):\n",
    "    return sum(lista) / len(lista)\n",
    "\n",
    "# Ejemplo de uso\n",
    "numeros = [4, 8, 15, 16, 23, 42]\n",
    "print(\"Promedio:\", promedio(numeros))\n"
   ]
  }
